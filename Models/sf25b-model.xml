<?xml version="1.0"?>
<!-- Falke Scheibe SF25-B
A. Goritsas-->

<PropertyList>
	<path>sf25b.ac</path>
	<texture-path>Textures</texture-path>
	<offsets>
		<x-m>0.0</x-m>
		<y-m>0.0</y-m>
		<z-m>0.0</z-m>

		<pitch-deg>0</pitch-deg>
		<roll-deg>0</roll-deg> 
	</offsets>
	<animation>
		<object-name>Rudder</object-name>
		<property>controls/flight/rudder</property>
		<type>rotate</type>
		<factor>45</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
		<center>
			<x-m>4.760</x-m>
			<y-m>0.0</y-m>
			<z-m>0.764</z-m>
		</center>
	</animation>
 	<animation>
		<type>rotate</type>
		<object-name>Rudderpedalspilot</object-name>
		<property>/controls/flight/rudder</property>
		<min-deg>-20</min-deg>
		<max-deg>20</max-deg>
		<factor>45</factor>
		<center>
			<x-m>-0.979</x-m>
			<y-m>-0.161</y-m>
			<z-m>-0.213</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
 	<animation>
		<type>rotate</type>
		<object-name>Rudderpedalscopilot</object-name>
		<property>/controls/flight/rudder</property>
		<min-deg>-20</min-deg>
		<max-deg>20</max-deg>
		<factor>45</factor>
		<center>
			<x-m>-1.030</x-m>
			<y-m>0.161</y-m>
			<z-m>-0.213</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<object-name>Rearwheel</object-name>
		<property>controls/flight/rudder</property>
		<factor>45</factor>
		<type>rotate</type>
		<factor>45</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
		<center>
			<x-m>4.679</x-m>
			<y-m>0.0</y-m>
			<z-m>0.026</z-m>
		</center>
	</animation>

	<animation>
		<object-name>Elevator</object-name>
		<object-name>Trim</object-name>
		<property>controls/flight/elevator</property>
		<type>rotate</type>
		<factor>45</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>4.357</x-m>
			<y-m>0.0</y-m>
			<z-m>0.477</z-m>
		</center>
	</animation>

 	<animation>
		<type>rotate</type>
		<object-name>Steeringstick</object-name>
		<property>/controls/flight/elevator</property>
		<min-deg>-15</min-deg>
		<max-deg>15</max-deg>
		<factor>18</factor>
		<center>
			<x-m>-0.696</x-m>
			<y-m>0</y-m>
			<z-m>-0.284</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

 	<animation>
		<type>rotate</type>
		<object-name>Steeringstick</object-name>
		<property>/controls/flight/aileron</property>
		<min-deg>-10</min-deg>
		<max-deg>10</max-deg>
		<factor>-10</factor>
		<center>
			<x-m>-0.696</x-m>
			<y-m>0</y-m>
			<z-m>-0.284</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<object-name>Trim</object-name>
  <property>controls/flight/elevator-trim</property>
  		<type>rotate</type>
  <factor>25</factor>	
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>4.604</x-m>
			<y-m>0.484</y-m>
			<z-m>0.476</z-m>
		</center>
	</animation>
	<animation>
		<object-name>Leftaileron</object-name>
		<property>controls/flight/aileron</property>
		<type>rotate</type>
		<factor>30</factor>
		<axis>
      	<x1-m>-0.069</x1-m>
      	<y1-m>-7.667</y1-m>
      	<z1-m>0.337</z1-m>
      	<x2-m>0.298</x2-m>
      	<y2-m>-4.761</y2-m>
      	<z2-m>0.166</z2-m>
    	</axis>
</animation>
<animation>
		<object-name>Rightaileron</object-name>
		<property>controls/flight/aileron</property>
		<type>rotate</type>
		<factor>-30</factor>
		<axis>
      	<x1-m>0.298</x1-m>
      	<y1-m>4.761</y1-m>
      	<z1-m>0.166</z1-m>
      	<x2-m>-0.069</x2-m>
      	<y2-m>7.667</y2-m>
      	<z2-m>0.337</z2-m>
    	</axis>
</animation>

<animation>
	<object-name>Leftspeedbrake</object-name>
	<type>rotate</type>
	<factor>-90</factor>
	<property>controls/flight/spoilers</property>
	<axis>
	 	<x1-m>-0.088</x1-m>
	 	<y1-m>-3.231</y1-m>
	 	<z1-m>0.213</z1-m>
	 	<x2-m>-0.023</x2-m>
	 	<y2-m>-2.141</y2-m>
	 	<z2-m>0.162</z2-m>
	</axis>
</animation>
	
<animation>
	<object-name>Rightspeedbrake</object-name>
	<type>rotate</type>
	<factor>-90</factor>
	<property>controls/flight/spoilers</property>
	<axis>
	 	<x1-m>-0.023</x1-m>
	 	<y1-m>2.141</y1-m>
	 	<z1-m>0.162</z1-m>
	 	<x2-m>-0.088</x2-m>
	 	<y2-m>3.231</y2-m>
	 	<z2-m>0.213</z2-m>
	</axis>
</animation>
<animation>
		<type>rotate</type>
		<object-name>Brakesticks</object-name>
		<property>/controls/flight/spoilers</property>
		<max-deg>30</max-deg>
		<factor>30</factor>
		<center>
			<x-m>-0.668</x-m>
			<y-m>-0.214</y-m>
			<z-m>-0.276</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>


<animation>
  <type>rotate</type>
  <object-name>canopyfront</object-name>
  <object-name>Canopyframe</object-name>
  <object-name>Canopybars</object-name>
  <property>controls/canopy/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
   <entry>
    <ind>1.00</ind>
    <dep>105.0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>-0.905</x-m>
   <y-m>0.0</y-m>
   <z-m>0.355</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>-1.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 
  <animation>
  <type>select</type>
  <object-name>Propeller</object-name>
  <condition>
   <less-than>
    <property>engines/engine[0]/rpm</property>
    <value>2000</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>Propeller</object-name>
  <property>engines/engine[0]/rpm</property>
  <factor>-1</factor>
  <center>
   <x-m>-1.949</x-m>
   <y-m>0.0</y-m>
   <z-m>0.139</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
